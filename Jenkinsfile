pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                echo 'build'
                echo 'Building..Building '
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
                echo 'test'
                echo 'Testing..Testing'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
                echo 'Deploy'
                echo 'Deploying.... Testing'
            }
        }
    }
}
